{
  "name": "react-rutter-link",
  "version": "1.2.0",
  "description": "A React component for Rutter Link",
  "files": [
    "dist",
    "src",
    "LICENSE"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "browser:min": "dist/index.umd.min.js",
  "browser": "dist/index.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rutterhq/react-rutter-link.git"
  },
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "react-component",
    "rutter"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rutter/react-rutter-link/issues"
  },
  "homepage": "https://github.com/rutter/react-rutter-link",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "lint": "eslint '{src,examples}/**/*.{ts,tsx,js,jsx}' --ignore-path .gitignore",
    "storybook": "start-storybook -p 9001",
    "deploy-storybook": "storybook-to-ghpages",
    "build-storybook": "build-storybook -s public",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "pub": "yarn build && yarn publish"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@rutter/rutter-link": "^1.2.0",
    "prop-types": "^15.7.2",
    "react-script-hook": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.7.0",
    "@types/enzyme": "^3.10.4",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "@wessberg/rollup-plugin-ts": "^1.2.21",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-typescript-to-proptypes": "^1.3.2",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.6.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.5.0",
    "husky": "^4.2.3",
    "mocha": "2.3.x",
    "parcel": "^2.0.0-beta.1",
    "prettier": "^1.19.1",
    "rollup": "^1.27.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "sinon": "1.17.x",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "xyz": "0.5.x"
  },
  "tags": [
    "react",
    "rutter"
  ]
}
